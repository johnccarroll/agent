{
  "cleanupPeriodDays": 30,
  "includeCoAuthoredBy": false,
  "env": {
    "CLAUDE_CODE_ENABLE_TELEMETRY": "0"
  },
  "permissions": {
    "defaultMode": "acceptEdits",
    "allow": [
      "Bash(*)",
      "Edit(*)",
      "Read(*)",
      "Write(*)",
      "MultiEdit(*)",
      "Glob(*)",
      "Grep(*)",
      "LS(*)",
      "TodoWrite(*)",
      "WebSearch(*)",
      "WebFetch(*)",
      "NotebookRead(*)",
      "NotebookEdit(*)",
      "Task(*)"
    ]
  }
}
