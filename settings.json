{
  "cleanupPeriodDays": 30,
  "includeCoAuthoredBy": true,
  "env": {
    "CLAUDE_CODE_ENABLE_TELEMETRY": "1"
  },
  "permissions": {
    "defaultMode": "allow",
    "allow": [
      "Bash(*)",
      "Edit(*)",
      "Read(*)",
      "Write(*)",
      "Glob(*)",
      "Grep(*)",
      "LS(*)",
      "TodoWrite(*)",
      "WebSearch(*)",
      "WebFetch(*)"
    ]
  }
}
